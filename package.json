{
	"name": "@mmstudio/base",
	"version": "1.0.0",
	"description": "$desc",
	"scripts": {
		"test": "npm run build && karma start --single-run",
		"dev": "tsc -w & karma start",
		"clean": "del-cli ./dist/",
		"lint": "eslint ./src/**/*.ts",
		"compile-amd": "tsc -m amd -d false --outFile ./dist/main.js",
		"compile-cjs": "tsc -m commonjs -d false",
		"compile-esm": "tsc && mv ./dist/index.js ./dist/index.esm.js",
		"compile": "npm run compile-esm && npm run compile-amd && npm run compile-cjs",
		"build": "npm run clean && npm run lint && npm run compile",
		"up": "git merge main"
	},
	"repository": {
		"type": "https+git",
		"url": "https://github.com/mm-core/base.git"
	},
	"homepage": "https://01factory.github.io",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"mm",
		"mmstudio",
		"01",
		"零壹",
		"微工厂",
		"原子操作",
		"控件",
		"框架"
	],
	"author": {
		"name": "$user",
		"email": "$email@xxx.com"
	},
	"maintainers": [
		{
			"name": "taoqf",
			"email": "tao_qiufeng@126.com"
		}
	],
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"chai": "latest",
		"del-cli": "latest",
		"eslint": "latest",
		"eslint-config-prettier": "latest",
		"eslint-config-ts-lambdas": "latest",
		"eslint-plugin-import": "latest",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "latest",
		"karma": "latest",
		"karma-chai": "latest",
		"karma-chrome-launcher": "latest",
		"karma-mocha": "latest",
		"karma-webpack": "latest",
		"mocha": "latest",
		"typescript": "next",
		"prettier": "^2.2.1",
		"webpack": "latest",
		"webpack-cli": "latest"
	}
}